      <Cell><Data ss:Type="String"><%= order.truck_reg_no %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.name %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.ic %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.phone_no %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.delivery_date %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.do_request_date %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.do_collection_date %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.do_number %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.total_tonne %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_name %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.tel_no %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.account_no %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.rate %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_rate %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.gst_total_value %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_with_gst %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_advance %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_paid %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_commision %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.payment_balance %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.billing_name %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.billing_rate %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.billing_amount %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.gst_billing_total_value %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.billing_with_gst %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.billing_received %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.billing_balance %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.invoice_date %></Data></Cell>
      <Cell><Data ss:Type="String"><%= order.invoice %></Data></Cell>
    </Row>
    <% end %>
  </Table>
</Worksheet>
</Workbook>


