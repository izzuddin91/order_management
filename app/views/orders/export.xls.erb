      <Cell><Data ss:Type="String">Pick up date</Data></Cell>
      <Cell><Data ss:Type="String">Goods Details</Data></Cell>
      <Cell><Data ss:Type="String">Pick up Address 1</Data></Cell>
      <Cell><Data ss:Type="String">Pick up PIC 1</Data></Cell>
      <Cell><Data ss:Type="String">Pick up Address 2</Data></Cell>
      <Cell><Data ss:Type="String">Pick up PIC 2</Data></Cell>
      <Cell><Data ss:Type="String">Pick up Address 3</Data></Cell>
      <Cell><Data ss:Type="String">Pick up PIC 3</Data></Cell>
